.container {
    width: 100%;
    height: 100%;
}

.page {
    min-height: 100%;
    flex: 1;
    background-color: #FBF9FE;
    font-size: 16px;
    font-family: -apple-system-font, Helvetica Neue, Helvetica, sans-serif;
    overflow: hidden;
}

.page__hd {
    padding: 40px;
}

.page__title {
    display: block;
    font-size: 20px;
}

.page__desc {
    display: block;
    margin-top: 5px;
    font-size: 14px;
    color: #888888;
}

.page__output {
    display: block;
    margin-top: 20px;
    font-size: 9px;
    color: #000000;
}

.page__bd {
    width: 100%;
}

.view {   
    width: 200px;
    height: 60px;
    margin: 10px auto;

    background:url(data:image/png;base64,);
}

.view1 {   
    background-size:30px 40px;
    background-repeat:no-repeat;
}

.view2 { 
    background-size:30% 100%;
    background-repeat:no-repeat;
}

.view3 { 
    background-size:100% 100%;
    padding:35px;
    border: 1px solid #ff00ff;
    background-origin:border-box;
    background-repeat:no-repeat;
}

.view4 { 
    background-size:100% 100%;
    padding:35px;
    border: 1px solid #ff00ff;
    background-origin:content-box;
    background-repeat:no-repeat;
}

.view5 {
    background:url(data:image/png;base64,), url(data:image/png;base64,);
}

.view11 {
    width: 240px;
    height: 60px;
    margin: 10px auto;
    background: linear-gradient(red, blue); 
}

.view12 {
    width: 240px;
    height: 60px;
    margin: 10px auto;
    background: linear-gradient(to right, red , blue);
}

.view13 {
    width: 240px;
    height: 60px;
    margin: 10px auto;
    background: linear-gradient(to bottom right, red , blue); 
}

.view14 {
    width: 240px;
    height: 60px;
    margin: 10px auto;
    background: linear-gradient(120deg, red, blue);
}

.view15 {
    width: 240px;
    height: 60px;
    margin: 10px auto;
    background: linear-gradient(red, green, blue);
}

.view16 {
    width: 240px;
    height: 60px;
    margin: 10px auto;
    background: linear-gradient(to right, red,orange,yellow,green,blue,indigo,violet); 
}

.view17 {
    width: 240px;
    height: 60px;
    margin: 10px auto;
    background: linear-gradient(to right, rgba(255,0,0,0), rgba(255,0,0,1)); 
}

.view18 {
    width: 240px;
    height: 60px;
    margin: 10px auto;
    background: repeating-linear-gradient(red, yellow 10%, green 20%);
}

.view21 {
    width: 120px;
    height: 120px;
    margin: 10px auto;
    background: radial-gradient(red, green, blue);
}

.view22 {
    width: 120px;
    height: 120px;
    margin: 10px auto;
    background: radial-gradient(red 5%, green 15%, blue 60%);
}

.view23 {
    width: 120px;
    height: 120px;
    margin: 10px auto;
    background: radial-gradient(circle, red, yellow, green); 
}

.view24 {
    width: 120px;
    height: 120px;
    margin: 10px auto;
    background: radial-gradient(60% 55%, closest-side,blue,green,yellow,black);
}

.view25 {
    width: 120px;
    height: 120px;
    margin: 10px auto;
    background: radial-gradient(60% 55%, farthest-side,blue,green,yellow,black);
}
.view26 {
    width: 120px;
    height: 120px;
    margin: 10px auto;
    background: repeating-radial-gradient(red, yellow 10%, green 15%);
}